<html xmlns="http://www.w3.org/1999/xhtml"  id="ROOT" >
  <head>
    <title>Snippet for annotation <!--$(id)--></title>
    <link rel="stylesheet" type="text/css" href="/data/css/lenses1.css"/> 

  </head>  
  <body>
    <div class="annotation-lens">
      <div class="annotation-title">
	<span class="CreationTime"><!--$(ctime)--></span>
	<span class="Author">Anonymous</span>
	<span><!--$(id)--></span>
	<a target="_blank" href="/?id_source=<!--$(id_source)-->&amp;page=<!--$(page)-->&amp;id_ann=<!--$(id)-->"><!--$(title)--></a> 
      </div>                           
      <div class = "annotation-body">
	<div class="exhibit-sel" style="<!--$(style)-->"></div>
	<table><tr>
	<td class="exhibit-tile"><img class="img-doc" src="/pdf/cache/<!--$(id_source)-->-<!--$(page)-->-0-<!--$(id_row)-->.png"> </img></td>
	<td class="exhibit-tile"><img class="img-doc" src="/pdf/cache/<!--$(id_source)-->-<!--$(page)-->-1-<!--$(id_row)-->.png"> </img></td>
	<td class="exhibit-tile"><img class="img-doc" src="/pdf/cache/<!--$(id_source)-->-<!--$(page)-->-2-<!--$(id_row)-->.png"> </img></td>
	<td class="exhibit-tile"><img class="img-doc" src="/pdf/cache/<!--$(id_source)-->-<!--$(page)-->-3-<!--$(id_row)-->.png"> </img></td>
	<td class="exhibit-tile"><img class="img-doc" src="/pdf/cache/<!--$(id_source)-->-<!--$(page)-->-4-<!--$(id_row)-->.png"> </img></td>
	</tr>	</table>
	<br/>

	<ul>
	  <li><span class="notebody"> <!--$(body)--></span></li>
	  <li><span class="speaker">Staff:</span> <span><!--$(comment)--></span> </li>
	</ul>
      </div>
    </div>
  </body>
</html>
